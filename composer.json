{
    "name": "linked/cakephp-cookie-auth",
    "description": "CookieAuth plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakephp/cakephp-codesniffer": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Linked\\CookieAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linked\\CookieAuth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
